{
    "editor.formatOnSave": true,
    "runOnSave.commands": [
        {
            "match": ".*\\.py",
            "runIn": "terminal",
            "command": "clear; python ${fileDirname}/tests.py  && git commit -am working  && git reset || git reset --hard"
        },
    ],
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./Problem8",
        "-p",
        "test*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.unittestEnabled": true,
    "python.autoComplete.extraPaths": [
        "./Problem1/solution.py",
        "./Problem2/solution.py",
        "./Problem3/solution.py",
        "./Problem4/solution.py",
        "./Problem5/solution.py",
        "./Problem6/solution.py",
        "./Problem7/solution.py",
        "./Problem8/solution.py",
        "./Problem9/solution.py",
        "./Problem10/solution.py"
    ],
    "python.analysis.extraPaths": [
        "./Problem1",
        "./Problem2",
        "./Problem3",
        "./Problem4",
        "./Problem5",
        "./Problem6",
        "./Problem7",
        "./Problem8",
        "./Problem9",
        "./Problem10"
    ],
}